{
  "functions": {
    "api/index.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/style.css",
      "dest": "/public/style.css"
    },
    {
      "src": "/script.js", 
      "dest": "/public/script.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}